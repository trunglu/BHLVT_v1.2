@using BHLVTBLL.Services.XConfig
@model Dictionary<string, Object>
@{
    if (Model.ContainsKey("web_view"))
    {
        WebView web_view = (WebView)((WebView)new WebView((WebView)Model["web_view"])
                            .SetWebViewPage(this)
                            .setPartialViewName("tab2_quantity")).Prepare();

        var dg2 = (WebDatagridClient)(web_view.CreateDatagrid()
                    .setClassDatagrid("easyui-datagrid")
                    .setUrlDataOption(Url.RouteUrl(new
                    {
                        controller = "Ultils",
                        action = "GetListInputData",
                        area = ""
                    }))
                   // Set id datagrid, remember this id for the process Jquery,(Javascript)
                   .setId("donhang_index_dg2")
                   .SetWebViewPage(this));
        <div>
            @Html.Raw(dg2.Widget())
        </div>
    }
}