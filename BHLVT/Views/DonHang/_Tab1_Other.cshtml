@using BHLVTBLL.Services.XConfig
@model Dictionary<string, object>
@{
    ViewBag.Title = "_Other";
    WebView web_view = new WebView((WebView)Model["web_view"]);
    web_view = (WebView)((WebView)(web_view)
                    .setPartialViewName("tab1_other")
                    .SetWebViewPage(this)).Prepare();
}
<div class="row clearfix">
    <div class="col-md-12">
        <form onsubmit="return false;" id="form-tab1-other">
            <div class="col-md-8 col-xs-8 col-sm-8">
                <div class="row">
                    <div class="col-md-4 col-sm-4">
                        <div class="form-group">
                            <label class="col-sm-8 col-md-8 control-label">@web_view.GetLabelInput("dh_dau_ki_tab1")</label>
                            <div class="col-md-4 col-sm-4 col-xs-4" style="padding:0px">
                                @Html.Raw(web_view.CreateTextInput("dh_dau_ki_tab1").Widget())
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-4 control-label">@web_view.GetLabelInput("tien_da_tt_tab1")</label>
                            <div class="col-md-8 col-sm-8 col-xs-8">
                                @Html.Raw(web_view.CreateTextInput("tien_da_tt_tab1").Widget())
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>

