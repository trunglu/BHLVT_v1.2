<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="~/Themes/XConfig/style.css" rel="stylesheet" />
    <link href="~/public/jq/themes/bootstrap/easyui.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/public/jq/themes/icon.css" />
    <title>@ViewBag.Title</title>
</head>

<body>
    <style>
        body {
            font-size: 12px
        }
    </style>
    <div id="container">
        @Html.Partial("PartialView/_Header")

        @Html.Partial("PartialView/_MenuMain")
        <div>
            @RenderBody()
        </div>
        <div class="clearfix"></div>

       
        @Html.Partial("PartialView/_Footer")
    </div>
    <script src="~/Themes/AdminLTE/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/Themes/AdminLTE/bower_components/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Themes/AdminLTE/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    @Scripts.Render("~/bundles/modules")
    @RenderSection("scripts", required: false)
</body>
</html>

